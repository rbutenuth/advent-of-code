; https://adventofcode.com/2021/day/1

(def list-of-strings (split (read-string-from-file "src/main/resources/2021/day01/task-input.txt") "\\r?\\n" 0))

(def list-of-numbers (map (lambda (x) (parse-number "#" "US" x)) list-of-strings)) 

(last (reduce 
	(lambda (acc value)
		(put last-value (first acc))
		(def sum (last acc))
		(if-else (and last-value (gt value last-value))
			(list value (+ 1 sum))
			(list value sum)
		)
	) 
	'(nil 0)
	list-of-numbers
))
